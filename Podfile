# platform :ios, '9.0'

target 'NotYoutube' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NotYoutube
  pod 'Firebase/Core'
  pod 'GoogleAPIClientForREST/YouTube', '~> 1.2.1'
  pod 'Google/SignIn', '~> 3.0.3'
  pod 'YoutubeKit'

end
